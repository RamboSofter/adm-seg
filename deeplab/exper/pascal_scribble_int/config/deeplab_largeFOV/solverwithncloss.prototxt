train_net: "${EXP}/config/${NET_ID}/trainwithncloss_${TRAIN_SET}.prototxt"

lr_policy: "step"
gamma: 0.1
stepsize: 2000
base_lr: 1e-4

random_seed: 10000000

display: 10
max_iter: 6000
momentum: 0.9
weight_decay: 0.0005

snapshot: 500
snapshot_prefix: "${EXP}/model/${NET_ID}/trainwithncloss${NC_LOSS_WEIGHT}_${TRAIN_SET}"
solver_mode: GPU
